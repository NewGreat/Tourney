        <level>Benutzersicht</level>
        <created>12-11-2014 12:10</created>
        <author>Fabian Biester</author>
        <lastUpdate>12-11-2014 14:39</lastUpdate>
        <primaryActorId>17</primaryActorId>
        <actors>
          <actorId>17</actorId>
        </actors>
        <mainScenario>
          <name/>
          <preConditions>
            <preCondition>
              <description>Anwendung wurde gestartet</description>
            </preCondition>
            <preCondition>
              <description>Der Nutzer hat ein Event erstellt und befindet sich in der Phase Voranmeldung</description>
            </preCondition>
            <preCondition>
              <description>Die Anwendung ist entsperrt</description>
            </preCondition>
            <preCondition>
              <description>Der &amp;#252;bermittelte Datensatz ist vollst&amp;#228;ndig</description>
            </preCondition>
          </preConditions>
          <attributes/>
          <steps>
            <step>
              <stepNumber>1</stepNumber>
              <actorId>17</actorId>
              <description>Dr&amp;#252;ckt den 'Spieler eintragen'-Button</description>
