      <useCase>
        <id>30</id>
        <name>Optionen, Passwort &amp;#228;ndern</name>
        <useCaseId>4</useCaseId>
        <goal>&lt;p style="margin-top: 0"&gt; Es soll ein neues Passwort gesetzt werden oder ein bestehendes ge&amp;#228;ndert werden &lt;/p&gt;</goal>
        <description>&lt;p style="margin-top: 0"&gt;
          Der Nutzer m&amp;#246;chte seine Anwendung mit einem Passwort sichern oder das 
          bestehende Passwort &amp;#228;ndern
          &lt;/p&gt;</description>
        <priority>Niedrig</priority>
        <level>Benutzersicht</level>
        <created>11-11-2014 12:50</created>
        <author>Fabian Biester</author>
        <lastUpdate>12-11-2014 12:31</lastUpdate>
        <primaryActorId>17</primaryActorId>
        <actors>
          <actorId>17</actorId>
        </actors>
        <mainScenario>
          <name/>
          <preConditions>
            <preCondition>
              <description>Anwendung ist gestartet</description>
            </preCondition>
            <preCondition>
              <description>Anwendung ist falls n&amp;#246;tig entsperrt</description>
            </preCondition>
          </preConditions>
          <attributes/>
          <steps>
            <step>
              <stepNumber>1</stepNumber>
              <actorId>17</actorId>
              <description>Nutzer dr&amp;#252;ckt den 'Optionen'-Button</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>2</stepNumber>
              <actorId/>
              <description>&amp;#214;ffnet Optionen-Fenster</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>3</stepNumber>
              <actorId>17</actorId>
              <description>Nutzer dr&amp;#252;ckt den 'Passwort &amp;#228;ndern'-Button</description>
              <references/>
