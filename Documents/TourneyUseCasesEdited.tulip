      <useCase>
        <id>34</id>
        <name>Turniermodul hinzuf&amp;#252;gen</name>
        <useCaseId>5</useCaseId>
        <goal>&lt;p style="margin-top: 0"&gt;
          Der Nutzer will eine Turniervariante erstellen
          &lt;/p&gt;</goal>
        <description>&lt;p style="margin-top: 0"&gt;
          Es wird ein neues Turniermodul erstellt, das weitgehend frei modifizierbar 
          ist und nachher als Vorlage f&amp;#252;r andere Events dienen kann
          &lt;/p&gt;</description>
        <priority>Niedrig</priority>
        <level>Benutzersicht</level>
        <created>12-11-2014 11:58</created>
        <author/>
        <lastUpdate>12-11-2014 12:10</lastUpdate>
        <primaryActorId>17</primaryActorId>
        <actors>
          <actorId>17</actorId>
        </actors>
        <mainScenario>
          <name/>
          <preConditions>
            <preCondition>
              <description>Tourney ist ge&amp;#246;ffnet</description>
            </preCondition>
            <preCondition>
              <description>Der Nutzer befindet sich auf der Startseite oder beim Erstellen eines Events in der Phase, in der die Turniere hinzugef&amp;#252;gt werden</description>
            </preCondition>
          </preConditions>
          <attributes/>
          <steps>
            <step>
              <stepNumber>1</stepNumber>
              <actorId>17</actorId>
              <description>Nutzer dr&amp;#252;ckt auf den 'Turniervorlage erstellen' Button</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>2</stepNumber>
              <actorId/>
              <description>Es &amp;#246;ffnet sich ein Fenster, in dem der Nutzer die spezifischen Einstellungen f&amp;#252;r das Turnier vornehmen kann</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>3</stepNumber>
              <actorId>17</actorId>
              <description>Stellt die Einstellung ein und dr&amp;#252;ckt daraufhin den 'Speichern'-Button</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>4</stepNumber>
              <actorId/>
              <description>Das Programm speichert die Einstellungen im Programmverzeichnis</description>
              <references/>
              <attributes/>
            </step>
          </steps>
          <postConditions>
            <postCondition>
              <description>Nutzer hat eine neue Vorlage f&amp;#252;r Turniere erstellt und kann diese in den Events verwenden</description>
            </postCondition>
          </postConditions>
        </mainScenario>
        <extensions>
          <extensionScenario>
            <associatedStep>4</associatedStep>
            <name>4a</name>
            <preConditions>
              <preCondition>
                <description>Die Vorlage kann nicht gespeichert werden</description>
              </preCondition>
            </preConditions>
            <attributes/>
            <steps>
              <step>
                <stepNumber>4a1</stepNumber>
                <actorId/>
                <description>Zeigt eine Warnung an, dass die &amp;#196;nderungen nicht gespeichert werden k&amp;#246;nnen</description>
                <references/>
                <attributes/>
              </step>
            </steps>
            <postConditions>
              <postCondition>
                <description>Der Nutzer wei&amp;#223;, dass seine &amp;#196;nderung nicht gespeichert wurde</description>
              </postCondition>
            </postConditions>
          </extensionScenario>
        </extensions>
      </useCase>
      <useCase>
        <id>38</id>
        <name>Voranmeldung</name>
        <useCaseId>6</useCaseId>
        <goal>&lt;p style="margin-top: 0"&gt;
          Voranmeldungen vor dem Event erfassen und speichern
          &lt;/p&gt;</goal>
        <description>&lt;p style="margin-top: 0"&gt;
          Der Nutzer hat von mindestens einem Spieler Voranmeldungen erhalten und 
