              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>4</stepNumber>
              <actorId/>
              <description>&amp;#220;berpr&amp;#252;ft, ob alle Angaben korrekt sind und tr&amp;#228;gt diese in die Datendank ein</description>
              <references/>
              <attributes/>
            </step>
          </steps>
          <postConditions>
            <postCondition>
              <description>Teilnehmer ist in Datenbank eingetragen</description>
            </postCondition>
          </postConditions>
        </mainScenario>
        <extensions>
          <extensionScenario>
