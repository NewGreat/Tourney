      <useCase>
        <id>15</id>
        <name>Event erstellen</name>
        <useCaseId>1</useCaseId>
        <goal>&lt;p style="margin-top: 0"&gt; Es kann mit der Voranmeldung begonnen werden, daher ist das Event mit allen notwendigen Daten fertig erstellt &lt;/p&gt;</goal>
        <description>&lt;p style="margin-top: 0"&gt; Es wird das Event erstellt und mit den vorhanden Daten modifiziert &lt;/p&gt;</description>
        <priority>Niedrig</priority>
        <level>Benutzersicht</level>
        <created>10-11-2014 18:17</created>
        <author>Fabian Biester</author>
        <lastUpdate>12-11-2014 21:41</lastUpdate>
        <primaryActorId>17</primaryActorId>
        <actors>
          <actorId>17</actorId>
        </actors>
        <mainScenario>
          <name/>
          <preConditions>
            <preCondition>
              <description>Anwendung wurde gestartet</description>
            </preCondition>
            <preCondition>
              <description>Anwendung wurde falls n&amp;#246;tig entsperrt</description>
            </preCondition>
          </preConditions>
          <attributes/>
          <steps>
            <step>
              <stepNumber>1</stepNumber>
              <actorId>17</actorId>
              <description>Nutzer startet das Erstellen des Events durch Dr&amp;#252;cken des Buttons "Event erstellen"</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>2</stepNumber>
              <actorId/>
              <description>&amp;#214;ffnet den Dialog f&amp;#252;r das Erstellen der Events</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>3</stepNumber>
              <actorId>17</actorId>
              <description>Nutzer vergibt Startdatum, Enddatum, Titel und &amp;#246;ffnet das Fenster um die Turniere hinzuzuf&amp;#252;gen</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>4</stepNumber>
              <actorId/>
              <description>Erstellt Datenbank f&amp;#252;r Vorabanmeldung</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>5</stepNumber>
              <actorId/>
              <description>&amp;#214;ffnet Dialog um Speicherort des Event zu bestimmen</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>6</stepNumber>
              <actorId>17</actorId>
              <description>Nutzer w&amp;#228;hlt den Speicherort aus</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>7</stepNumber>
              <actorId/>
              <description>Speichert das Event an dem vogegebenen Ort</description>
              <references/>
              <attributes/>
            </step>
          </steps>
          <postConditions>
            <postCondition>
              <description>Vorabanmeldung kann gestartet werden</description>
            </postCondition>
            <postCondition>
              <description>Event wurde gespeichert</description>
            </postCondition>
          </postConditions>
        </mainScenario>
        <extensions>
          <extensionScenario>
            <associatedStep>3</associatedStep>
            <name>3a</name>
            <preConditions>
              <preCondition>
                <description>Standard-Module sind nicht vorhanden</description>
              </preCondition>
            </preConditions>
            <attributes/>
            <steps>
              <step>
                <stepNumber>3a1</stepNumber>
                <actorId/>
                <description>Gibt eine Meldung aus, dass die Standard-Module nicht gefunden wurden und verweist auf das Erstellen eines neuen Turniermoduls</description>
                <references/>
                <attributes/>
              </step>
              <step>
                <stepNumber>3a2</stepNumber>
                <actorId>17</actorId>
                <description>Erstellt ein Turniermodul (siehe Use Case 5: Turniermodul erstellen)</description>
                <references/>
                <attributes/>
              </step>
              <step>
                <stepNumber>3a3</stepNumber>
                <actorId>17</actorId>
                <description>Der Nutzer f&amp;#252;gt dieses daraufhin dem Event hinzu</description>
                <references/>
                <attributes/>
              </step>
            </steps>
            <postConditions>
              <postCondition>
                <description>Das Event enth&amp;#228;lt nun ein oder mehrere Turniere</description>
              </postCondition>
            </postConditions>
          </extensionScenario>
          <extensionScenario>
            <associatedStep>3a2</associatedStep>
            <name>3a2a</name>
            <preConditions>
              <preCondition>
                <description>Schlie&amp;#223;t Dialog ohne ein neues Turniermodul zu erstellen</description>
              </preCondition>
            </preConditions>
            <attributes/>
            <steps>
              <step>
                <stepNumber>3a2a1</stepNumber>
                <actorId/>
                <description>Geht in Zustand Y &amp;#252;ber</description>
                <references/>
                <attributes/>
              </step>
            </steps>
            <postConditions>
              <postCondition>
                <description>Zustand Y</description>
              </postCondition>
            </postConditions>
          </extensionScenario>
          <extensionScenario>
            <associatedStep>7</associatedStep>
            <name>7a</name>
            <preConditions>
              <preCondition>
                <description>Ungen&amp;#252;gende Berechtigung um in den Zielordner zu schreiben</description>
              </preCondition>
            </preConditions>
            <attributes/>
            <steps>
              <step>
                <stepNumber>7a1</stepNumber>
                <actorId/>
                <description>Gibt eine Warnung aus, dass eine ungen&amp;#252;gende Schreibberechtigung vorhanden ist</description>
                <references/>
                <attributes/>
              </step>
            </steps>
            <postConditions>
              <postCondition>
                <description>Nutzer wurde infomiert, dass er ein neues Speicherziel w&amp;#228;hlen muss oder seine &amp;#196;nderungen werden nicht gespeichert</description>
              </postCondition>
            </postConditions>
          </extensionScenario>
        </extensions>
      </useCase>
