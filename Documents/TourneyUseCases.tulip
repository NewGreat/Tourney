      <useCase>
        <id>15</id>
        <name>Event erstellen</name>
        <useCaseId>1</useCaseId>
        <goal>&lt;p style="margin-top: 0"&gt; Es kann mit der Voranmeldung gestartet werden, daher das Event is fertig erstellt mit allen notwendigen Daten &lt;/p&gt;</goal>
        <description>&lt;p style="margin-top: 0"&gt; Es wird das Event erstellt und mit den vorhanden Daten modifiziert &lt;/p&gt;</description>
        <priority>Niedrig</priority>
        <level>Benutzersicht</level>
        <created>10-11-2014 18:17</created>
        <author>Fabian Biester</author>
        <lastUpdate>12-11-2014 21:41</lastUpdate>
        <primaryActorId>17</primaryActorId>
        <actors>
          <actorId>17</actorId>
        </actors>
        <mainScenario>
          <name/>
          <preConditions>
            <preCondition>
              <description>Anwendung wurde gestartet.</description>
            </preCondition>
            <preCondition>
              <description>Anwendung wurde entsperrt, falls nötig.</description>
            </preCondition>
          </preConditions>
          <attributes/>
          <steps>
            <step>
              <stepNumber>1</stepNumber>
              <actorId>17</actorId>
              <description>Nutzer startet das Erstellen des Events durch Drücken des Buttons "Event erstellen"</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>2</stepNumber>
              <actorId/>
              <description>Öffnet den Dialog für das Erstellen der Events</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>3</stepNumber>
              <actorId>17</actorId>
              <description>Nutzer gibt Startdatum, Enddatum, Titel und öffnet das Fenster um die Tuniere hinzuzufügen. </description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>4</stepNumber>
              <actorId/>
              <description>Erstellt Datenbank für Vorabanmeldung.</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>5</stepNumber>
              <actorId/>
              <description>Öffnet Dialog um Speicherort des Event zu bestimmen.</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>6</stepNumber>
              <actorId>17</actorId>
              <description>Nutzer wählt den Speicherort aus.</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>7</stepNumber>
              <actorId/>
              <description>Speichert das Event an dem vogegebenen Ort.</description>
              <references/>
              <attributes/>
            </step>
          </steps>
          <postConditions>
            <postCondition>
              <description>Vorabanmeldung kann gestartet werden</description>
            </postCondition>
            <postCondition>
              <description>Event wurde gespeichert.</description>
            </postCondition>
          </postConditions>
        </mainScenario>
        <extensions>
          <extensionScenario>
            <associatedStep>3</associatedStep>
            <name>3a</name>
            <preConditions>
              <preCondition>
                <description>Standard Module sind nicht vorhanden.</description>
              </preCondition>
            </preConditions>
            <attributes/>
            <steps>
              <step>
                <stepNumber>3a1</stepNumber>
                <actorId/>
                <description>Gibt eine Meldung aus, dass die Standard Module nicht gefunden wurde und verweißt auf das Erstellen eines neuen Tuniermoduls</description>
                <references/>
                <attributes/>
              </step>
              <step>
                <stepNumber>3a2</stepNumber>
                <actorId>17</actorId>
                <description>Erstellt ein Tuniermodul (siehe Use Case x Tuniermodul erstellen)</description>
                <references/>
                <attributes/>
              </step>
              <step>
                <stepNumber>3a3</stepNumber>
                <actorId>17</actorId>
                <description>Der Nutzer fügt dieses daraufhin dem Event hinzu.</description>
                <references/>
                <attributes/>
              </step>
            </steps>
            <postConditions>
              <postCondition>
                <description>Das Event enthält nun ein oder mehrere Tuniere.</description>
              </postCondition>
            </postConditions>
          </extensionScenario>
          <extensionScenario>
            <associatedStep>3a2</associatedStep>
            <name>3a2a</name>
            <preConditions>
              <preCondition>
                <description>Schließt Dialog ohne ein neues Tuniermodul zu erstellen</description>
              </preCondition>
            </preConditions>
            <attributes/>
            <steps>
              <step>
                <stepNumber>3a2a1</stepNumber>
                <actorId/>
                <description>Geht in Zustand Y über</description>
                <references/>
                <attributes/>
              </step>
            </steps>
            <postConditions>
              <postCondition>
                <description>Zustand Y</description>
              </postCondition>
            </postConditions>
          </extensionScenario>
          <extensionScenario>
            <associatedStep>7</associatedStep>
            <name>7a</name>
            <preConditions>
              <preCondition>
                <description>Ungenügende Berechtigung im Ziel Ordner zu schreiben</description>
              </preCondition>
            </preConditions>
            <attributes/>
            <steps>
              <step>
                <stepNumber>7a1</stepNumber>
                <actorId/>
                <description>Gibt eine Warnung aus das die ungenügende Schreibberechtigungen vorhanden sind.</description>
                <references/>
                <attributes/>
              </step>
            </steps>
            <postConditions>
              <postCondition>
                <description>Nutzer wurde infomiert das er ein neues Speicherziel wählen muss oder seine Änderungen werden nicht gespeichert.</description>
              </postCondition>
            </postConditions>
          </extensionScenario>
        </extensions>
      </useCase>
      <useCase>
