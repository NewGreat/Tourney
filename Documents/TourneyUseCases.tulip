      <useCase>
        <id>34</id>
        <name>Tuniermodul hinzufügen</name>
        <useCaseId>5</useCaseId>
        <goal>&lt;p style="margin-top: 0"&gt;
          Der Nutzer will eine Tuniervariante erstellen.
          &lt;/p&gt;</goal>
        <description>&lt;p style="margin-top: 0"&gt;
          Es wird ein neues Tuniermodul erstellt das weitgehend frei modifizierbar 
          ist und nachher als Vorlage f&amp;#252;r andere Events dienen kann.
          &lt;/p&gt;</description>
        <priority>Niedrig</priority>
        <level>Benutzersicht</level>
        <created>12-11-2014 11:58</created>
        <author/>
        <lastUpdate>12-11-2014 12:10</lastUpdate>
        <primaryActorId>17</primaryActorId>
        <actors>
          <actorId>17</actorId>
        </actors>
        <mainScenario>
          <name/>
          <preConditions>
            <preCondition>
              <description>Tourney ist geöffnet.</description>
            </preCondition>
            <preCondition>
              <description>Der Nutzer befindet sich auf der Startseite oder beim erstellen eines Events in der Phase, in der die Tuniere hinzugefügt werden.</description>
            </preCondition>
          </preConditions>
          <attributes/>
          <steps>
            <step>
              <stepNumber>1</stepNumber>
              <actorId>17</actorId>
              <description>Nutzer drückt auf den 'Tuniervorlage erstellen' Button.</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>2</stepNumber>
              <actorId/>
              <description>Es öffnet sich ein Fenster, indem der Nutzer die spezifischen Einstellungen für das Tunier einstellen kann.</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>3</stepNumber>
              <actorId>17</actorId>
              <description>Stellt die Einstellung ein und drückt daraufhin auf 'Speichern' Button.</description>
              <references/>
              <attributes/>
            </step>
            <step>
              <stepNumber>4</stepNumber>
              <actorId/>
              <description>Das Programm speichert die Einstellungen im Programm Verzeichnis.</description>
              <references/>
              <attributes/>
            </step>
          </steps>
          <postConditions>
            <postCondition>
              <description>Nutzer hat eine neue Vorlage für Tuniere erstellt und kann diese in den Events verwenden.</description>
            </postCondition>
          </postConditions>
        </mainScenario>
        <extensions>
          <extensionScenario>
            <associatedStep>4</associatedStep>
            <name>4a</name>
            <preConditions>
              <preCondition>
                <description>Die Vorlage kann nicht gespeichert werden.</description>
              </preCondition>
            </preConditions>
            <attributes/>
            <steps>
              <step>
