<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import usspg31.tourney.controller.layout.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<fx:root alignment="TOP_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" spacing="5.0" type="VBox"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<HBox spacing="10.0" VBox.vgrow="ALWAYS">
		<IconPane styleClass="icon-victory, left" />
		<VBox spacing="5.0" HBox.hgrow="ALWAYS" alignment="CENTER">
			<Label fx:id="labelTournamentName" styleClass="big-heading" text="Deutsche Meisterschaft" alignment="CENTER" />
			<Pane minHeight="50.0" />
			<Label styleClass="sub-heading" text="%victorydialog.winner" alignment="CENTER" />
			<Label fx:id="labelWinner" styleClass="medium-heading" alignment="CENTER" />
			<Pane minHeight="30.0" />
		</VBox>
		<IconPane styleClass="icon-victory, right" />
	</HBox>
</fx:root>
